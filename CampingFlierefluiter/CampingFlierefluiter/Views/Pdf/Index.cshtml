@model IEnumerable<CampingFlierefluiter.Models.PdfModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.pdfTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pdflocation)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.pdfTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pdflocation)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.pdfId }) |
            @Html.ActionLink("Details", "Details", new { id=item.pdfId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.pdfId })
        </td>
    </tr>
}

</table>
